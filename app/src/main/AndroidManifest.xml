<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tm.allfunctions">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".com.example.tm.allfunctions.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".com.example.tm.allfunctions.widget.AsyncActivity"
            android:label="@string/title_activity_async"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".com.example.tm.allfunctions.main.ExplicitActivity" />
        <activity android:name=".com.example.tm.allfunctions.main.Explicit2Activity" />
        <activity android:name=".com.example.tm.allfunctions.main.ImplicitActivity" />
        <activity android:name=".com.example.tm.allfunctions.main.WidgetActivity" />
        <activity android:name=".com.example.tm.allfunctions.widget.FloatingButtonActivity" />
        <activity android:name=".com.example.tm.allfunctions.widget.SpinnerActivity" />
        <activity android:name=".com.example.tm.allfunctions.widget.SeekBarActivity" />
        <activity android:name=".com.example.tm.allfunctions.widget.ProgressBarActivity" />
        <activity android:name=".com.example.tm.allfunctions.widget.AlertDialogActivity" />
        <activity android:name=".com.example.tm.allfunctions.main.SplashActivity" />
        <activity android:name=".com.example.tm.allfunctions.main.CameraActivity"></activity>
    </application>

</manifest>